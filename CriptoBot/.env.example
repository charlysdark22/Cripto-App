# Environment Configuration for CriptoBot
# Copiar este archivo a .env.local y configurar seg√∫n tu entorno

# API Configuration
API_BASE_URL=http://localhost:5000/api
API_TIMEOUT=10000

# Environment
ENVIRONMENT=development
LOG_LEVEL=debug

# Feature Flags
ENABLE_ANALYTICS=true
ENABLE_CRASH_REPORTING=false
ENABLE_DEBUG_MENU=true

# Broker Configuration (estos se configurable desde la app)
# BROKER_TYPE=interactive-brokers
# BROKER_API_KEY=your_api_key
# BROKER_API_SECRET=your_api_secret

# App Version
APP_VERSION=1.0.0
BUILD_NUMBER=1

# Firebase (opcional, para notificaciones)
# FIREBASE_PROJECT_ID=your_project_id
# FIREBASE_MESSAGING_SENDER_ID=your_sender_id
# FIREBASE_API_KEY=your_api_key
